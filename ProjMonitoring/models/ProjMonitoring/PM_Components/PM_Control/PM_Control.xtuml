INSERT INTO SPR_PS
	VALUES ("c67f3b56-9d25-47ff-a6f0-519e86c0ef91",
	'add_Program',
	'This signal incorporates not only a new program identifier, but also a strategic goal idetifier.
Mainly to ensure that the PROGRAM is associated with the correct STRATEGIC GOAL right from get-go.

Thus the basic aspects of all UC01, UC06 and and UC01-1 are incorporated here.',
	'//Creating a new program requires 8 inputs. 
//Call the Program class operation named Create.

PRG::Create( PRG_Name : param.Prog_Name, Description : param.Description, Budget : param.Budget, Manager : param.Manager );

// Set Start and End Dates
PRG::add_Start_DMY( PRG_Name: param.Prog_Name, Day: TIM::get_day( date: param.Start_Date), Month: TIM::get_month( date: param.Start_Date), Year: TIM::get_year( date: param.Start_Date) );
PRG::add_End_DMY( PRG_Name: param.Prog_Name, Day: TIM::get_day( date: param.End_Date), Month: TIM::get_month( date: param.End_Date), Year: TIM::get_year( date: param.End_Date) );

// Link to Startegic Goal
PRG::link_To_Startegic_Goal( PRG_Name: param.Prog_Name, SG_Name: param.Goal_ID );

// Send a notification of the creation of a new program to a manager within Program_Management.

send PrgMan::Notify( Relevant_Person:"Manager", Message:"New Program Created" );',
	1,
	0,
	1);
