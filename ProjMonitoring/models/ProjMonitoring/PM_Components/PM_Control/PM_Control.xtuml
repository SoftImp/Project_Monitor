INSERT INTO SPR_PS
	VALUES ("01af2a1a-d759-4ae1-9374-434c0ae80098",
	'input_EV_Data',
	'',
	'//A Project Manager (or representative) needs to provide basic Earned Value data to instantiate a new Performance Report.
//Such input could be required )and input) at any time during the course of a project, but usually at articulary ''milestones''.
//All varienaces, indices etc. for a project are calculated automatically after the input has been provided.

PERFREP::Create (PRJ_Name : param.PRJ_Name, BAC : param.BAC, EV : param.EV, PV : param.PV, AC : param.AC);

