INSERT INTO O_TFR
	VALUES ("3aa837c5-b6d0-4239-9e73-04f51ca61f56",
	"ebd48d02-0571-4979-8fde-ce5707451104",
	'link_To_Startegic_Goal',
	'',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'//Associate/Link Portfolio to a Strategic Goal.
//Based on UC01-1.

select any Portfolio from instances of PF
 where (selected.PF_Name == param.PF_Name);
 
if (not empty Portfolio)

	select any Strategic_Goal from instances of SG
	 where (selected.SG_Name == param.SG_Name);

	if ( not empty Strategic_Goal)
	
		relate self to Strategic_Goal across R1.''aligns with'';
	
		LOG::LogInfo( message: "A PORTFOLIO has been aligned with a STRATEGIC GOAL." );
		send OrgMan::Notify(Relevant_Person:"Manager", Message:"A PORTFOLIO  " + param.PF_Name + "has been aligned with a STRATEGIC GOAL.");
	else
		LOG::LogFailure( message: "The chosen STRATEGIC GOAL does not exist.  Please choose a relevant GOAL that already exists." );
	end if;
else
	LOG::LogFailure( message: "The chosen PORTFOLIO NAME doesn''t exist.  Please choose a NAME that already exists." );
end if;',
	1,
	'',
	"c4773d6a-6a99-4902-889b-f32228e79251",
	0,
	6);
