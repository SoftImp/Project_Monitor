INSERT INTO O_TFR
	VALUES ("da85f395-9780-429b-aec8-86293893ff00",
	"fc2339db-f29a-4ff9-915d-62c8814fc04c",
	'update_Requirement_ID',
	'',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'//An operation that allows an initial requirement identifier containing "TBA for" to be updated.

//First select the named Program

//select any Program from instances of PRG
//where (selected.PRG_Name == param.PRG_Name);
 
// if (not empty Program)
 	
 	//If the Program exists then select the related Change Request.

	select any Request related by self->PRCR[R13]
	 where (selected.PRCR_ID == param.Request_ID);

	if ( not empty Request)
		
		//If the Change Request exists then select the related Requirement
		
		select any Requirement related by self->PRGREQ[R13]
		where (selected.Req_ID == ("TBA for " + param.Request_ID));
		
		if ( not empty Requirement)
			
			//If the (unchanged) Requirement exists then rename it.
		
			Requirement.Req_ID = param.New_Req_ID;
			
			send PrgMan::Notify( Relevant_Person: "Manager", Message: "The Requirement ID for CHANGE REQUEST " + param.Request_ID + " has been updated to " + param.New_Req_ID);
		
		else
			
			LOG::LogInfo( message: "The identified REQUIREMENT either doesn''t exist or it''s initial identity has already been changed." );
			
		end if;
		
