INSERT INTO O_TFR
	VALUES ("83ff4db1-4f38-42cf-bb30-f59aeb85b722",
	"fc2339db-f29a-4ff9-915d-62c8814fc04c",
	'update_Requirement_Description',
	'',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'//An operation that allows a requirement description to be updated.
//Assume that the initial Req_ID containing "TBA for" has been updated or needs to be updated.

//First select the named Program

//select any Program from instances of PRG
 //where (selected.PRG_Name == param.PRG_Name);
 
 //if (not empty Program)
 	
 	//If the Program exists then determine if the chosen requirement has derived from a change request.

	select any Requirement related by self->PRGREQ[R13]
	where (selected.Req_ID == param.Req_ID);
		
	if ( not empty Requirement)
			
		//Update the Requirement.
		
		Requirement.Description = param.Description;
		
		LOG::LogInfo( message: "The Description for REQUIREMENT " + param.Req_ID + " has been updated." );
			
		send PrgMan::Notify( Relevant_Person: "Manager", Message: "The Requirement Description for " + param.Req_ID + " has been updated.");
		
	else
			
			LOG::LogInfo( message: "The identified REQUIREMENT either doesn''t exist or it''s initial identity hasn''t been changed." );
			
	end if;

	 //If the Program exists then determine if the requirement hasn''t derived from a change request.	
	
	select any Requirement related by self->PRGREQ[R12.''is to implement'']
	where (selected.Req_ID == param.Req_ID);
	
	if ( not empty Requirement)
			
		//Update the Requirement.
		
		Requirement.Description = param.Description;
		
		LOG::LogInfo( message: "The Description for REQUIREMENT " + param.Req_ID + " has been updated." );
			
		send PrgMan::Notify( Relevant_Person: "Manager", Message: "The Requirement ID for CHANGE REQUEST " + param.Req_ID + " has been updated.");
		
	else			
		
		LOG::LogFailure( message: "The chosen REQUIREMENT doesn''t exist.  Please choose a NAME that already exists." );

	end if;
	
//else
	
	//LOG::LogFailure( message: "The chosen PROGRAM NAME doesn''t exist.  Please choose a NAME that already exists." );
	
//end if;',
	1,
	'',
	"da85f395-9780-429b-aec8-86293893ff00",
	0,
	1);
