INSERT INTO O_TFR
	VALUES ("ad55ed0a-6904-4a72-98a6-50d60bf09eac",
	"85b0628f-8218-4afe-9117-bf931890238c",
	'get_Projects',
	'',
	"ba5eda7a-def5-0000-0000-000000000004",
	0,
	'if (param.PRJ_Name != "")
	select any p from instances of PRJ
 	 where (selected.PRJ_Name == param.PRJ_Name);
 	 
 	if (not empty p)
 		LOG::LogInfo( message: "prg: " + p.PRJ_Name + " " + p.Description );
 	end if;
else 		
	select many projects from instances of PRJ;
	for each p in projects
	  LOG::LogInfo( message: "prg: " + p.PRJ_Name + " " + p.Description );
	
	  select one goal related by p->SG[R3.''may be driven by''];
	  if ( not empty goal )
	  	LOG::LogInfo( message: "g: " + goal.SG_Name );
	  end if;
	    
	  select one prg related by p->PRG[R5.''may be part of''];
	  if ( not empty prg )
	  	LOG::LogInfo( message: "prg: " + prg.PRG_Name );
	  end if;  
	  
	  select one pf related by p->PF[R6.''may be part of''];
	  if ( not empty pf )
	  	LOG::LogInfo( message: "pf: " + pf.PF_Name );
	  end if;
	  
	  select many reports related by p->PERFREP[R14.''may be recorded in''];	
	  for each rep in reports
	  	LOG::LogInteger( message: rep.Rep_ID );
	  end for;		 	
	end for;
end if;	

return "";',
