INSERT INTO O_TFR
	VALUES ("23fe229e-ea01-45f8-bfe7-03a840fd662c",
	"85b0628f-8218-4afe-9117-bf931890238c",
	'get_Perf_Rep',
	'',
	"ba5eda7a-def5-0000-0000-000000000004",
	0,
	'//Get a performance report for a project by Rep_ID.

//Check that the named project exists.

select any Project from instances of PRJ
 where (selected.PRJ_Name == param.PRJ_Name);

if (not empty Project)
  	select many reports related by Project->PERFREP[R14.''may be recorded in''];	
  	for each rep in reports
  		if (rep.Rep_ID == param.Rep_ID)
  			LOG::LogInteger( message: rep.Rep_ID );
  			break;
  		end if;
	end for;
else
	LOG::LogFailure( message: "The chosen PROJECT " + param.PRJ_Name + " does not exist.  Please choose a PROJECT NAME that already exists." );
end if;

return "";',
	1,
	'',
	"ad55ed0a-6904-4a72-98a6-50d60bf09eac",
	0,
	5);
